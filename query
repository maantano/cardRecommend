CREATE TABLE benefit (
 id        INT NOT NULL AUTO_INCREMENT,
 name      VARCHAR(20),
 detailItem    VARCHAR(100),
  PRIMARY KEY(id)
) ENGINE=MYISAM CHARSET=utf8;

