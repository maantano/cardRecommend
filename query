CREATE TABLE `child` (
	`id` INT(11) NOT NULL,
	`id1` INT(11) NOT NULL,
	`id2` INT(11) NOT NULL,
	`uk1` INT(11) NOT NULL,
	`uk2` INT(11) NOT NULL,
	PRIMARY KEY (`id`),
	FOREIGN KEY (`id1`, `id2`) REFERENCES `parent` (`id1`, `id2`),
	FOREIGN KEY (`uk1`, `uk2`) REFERENCES `parent` (`uk1`, `uk2`)
)

CREATE TABLE `parent` (
	`id1` INT(11) NOT NULL,
	`id2` INT(11) NOT NULL,
	`id3` INT(11) NOT NULL,
	`uk1` INT(11) NOT NULL,
	`uk2` INT(11) NOT NULL,
	`uk3` INT(11) NOT NULL,
	PRIMARY KEY (`id1`, `id2`, `id3`),
	UNIQUE KEY (`uk1`, `uk2`, `uk3`)
);
-- detailBenefit
CREATE TABLE cardAll (
 id        VARCHAR(10) NOT NULL,
 cardType     VARCHAR(10) NOT NULL,
 benefit    VARCHAR(10),
 eventYN   VARCHAR(10) NOT NULL,
 cardCo   VARCHAR(10) not null,
 cardName   VARCHAR(50) not null,
 cardImg   VARCHAR(200) not null,
 eventTitle   VARCHAR(50),
 benefitT1   VARCHAR(10),
 benefitC1   VARCHAR(10),
 benefitT2   VARCHAR(10),
 benefitC2   VARCHAR(10),
 benefitT3   VARCHAR(10),
 benefitC3   VARCHAR(10),
 nationalType   VARCHAR(40),
 previousPer   VARCHAR(20),
 issueType   VARCHAR(15),
 globalMerchant   VARCHAR(20),
 urlLink   VARCHAR(30),
 annualFee int,
  PRIMARY KEY(id)
)ENGINE=InnoDB;

ALTER TABLE cardAll ADD COLUMN eventType varchar(20) AFTER eventYN;
ALTER TABLE cardBenefit ADD COLUMN summaryCategory varchar(200) AFTER detailItem;

-- detailBenefit

-- detailBenefit
create table detailBenefit(
  id VARCHAR(10) not null,
  benefitCode VARCHAR(200) not null,
  benefitTitle text not null,
  benefitSubTitle text not null,
  benefitContents text not null,
  PRIMARY KEY(id)
  FOREIGN KEY ('id') REFERENCES `card` (`id`)
  FOREIGN KEY ('benefitCode') REFERENCES `cardBenefit` (`id`)
)
-- detailBenefit

create table mobile04Rank(
  id VARCHAR(10) not null,
  benefitCode VARCHAR(20) not null,
  benefitTitle VARCHAR(20) not null,
  benefitRank int not null,
  PRIMARY KEY(id)
  FOREIGN KEY ('id') REFERENCES `cardAll` (`id`)
  FOREIGN KEY ('benefitCode') REFERENCES `cardBenefit` (`id`)
)

